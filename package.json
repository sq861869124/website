{
  "name": "erda-website",
  "version": "1.0.0",
  "description": "Erda Cloud website",
  "keywords": [
    "Erda",
    "Cloud",
    "website"
  ],
  "homepage": "https://github.com/erda-project/website#readme",
  "bugs": {
    "url": "https://github.com/erda-project/website/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erda-project/website.git"
  },
  "license": "ISC",
  "author": "Erda-FE",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/webpack/bin/webpack.js serve --config webpack.server.js",
    "watch": "rm -rf public && webpack --progress --color -w",
    "build": "rm -rf public && npm run build-site",
    "build-site": "NODE_ENV=production webpack --progress --color",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "eslint-fix": "eslint --fix 'src/**/*.tsx'",
    "stylelint:fix": "stylelint 'src/**/*.scss' --config .stylelintrc.json --fix --syntax scss && npm run prettier:scss",
    "prettier:scss": "prettier -c --write \"src/**/*.scss\"",
    "release": "shell/build.sh",
    "analyze": "rm -rf public && NODE_ENV=production webpack --config webpack.analyze.js",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "erda check-license -d './src'",
    "src/**/*.scss": "npm run stylelint:fix",
    "src/**/*.{md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "ace-builds": "^1.4.12",
    "animate.css": "^4.1.1",
    "antd": "^4.15.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "clipboard": "^2.0.8",
    "cube-state": "^1.2.3",
    "highlight.js": "^10.7.2",
    "intersection-observer": "^0.12.0",
    "lodash": "^4.17.21",
    "marked": "^2.0.1",
    "mock-json-schema": "^1.0.8",
    "path-to-regexp": "^6.2.0",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-ace": "^9.4.0",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "^17.0.2",
    "react-lazyload": "^3.2.0",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.2.3",
    "resolve-pathname": "^3.0.0",
    "screenfull": "^5.1.0",
    "swagger-parser": "^10.0.2",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@commitlint/read": "^12.1.1",
    "@principalstudio/html-webpack-inject-preload": "^1.2.7",
    "@svgr/webpack": "^5.5.0",
    "@terminus/lint-config": "^0.1.2",
    "@types/classnames": "^2.2.11",
    "@types/clipboard": "^2.0.1",
    "@types/highlight.js": "^10.1.0",
    "@types/lodash": "^4.14.168",
    "@types/marked": "^2.0.1",
    "@types/node": "^14.14.37",
    "@types/qrcode.react": "^1.0.1",
    "@types/react-animate-on-scroll": "^2.1.2",
    "@types/react-lazyload": "^3.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@webpack-cli/serve": "^1.3.1",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.23.0",
    "eslint-config-ali": "^12.0.1",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "file-loader": "^6.2.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-webpack-plugin": "^5.3.1",
    "https-browserify": "^1.0.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.4.1",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "process": "^0.11.10",
    "rucksack-css": "^1.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "sass-resources-loader": "^2.1.1",
    "stream-http": "^3.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "thread-loader": "^3.0.1",
    "ts-import-plugin": "^1.6.7",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.3",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^5.30.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-spritesmith": "^1.1.0"
  }
}
