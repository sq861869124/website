{
  "extends": ["stylelint-config-rational-order"],
  "ignoreFiles": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx"
  ],
  "rules": {
    "rule-empty-line-before": [
      "always",
      {
        "except": ["first-nested", "after-single-line-comment"]
      }
    ],
    "selector-max-compound-selectors": [10]
  }
}
